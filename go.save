#!/bin/bash  

if [ -n "$1" ]; then
    simnum=$1
else
    echo "must have simnum"
    exit -1
fi

echo $simnum

for i in density velocity amps speed ; do
    echo $i
    rm simulation/*
    rm cropped/*
    sleep 1
    mv $i/* simulation
    sleep 1
    ./makemovie.wire.nc.py
    ./go.save1 $simnum $i
done

